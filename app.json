{
  "expo": {
    "name": "Trimo - trabalhador",
    "slug": "trimo-trabalhador",
    "platforms": ["ios", "android"],
    "version": "0.3.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.citrussinensis.fielduser"
    },
    "android": {
      "package": "com.citrussinensis.fielduser",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}
